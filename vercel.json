{
  "version": 2,
  "framework": "vite",
  "devCommand": "npm run dev",
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "client/dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/properties(/(.*))?",
      "destination": "/api/properties.ts"
    },
    {
      "source": "/api/sitemap(/(.*))?",
      "destination": "/api/sitemap.ts"
    },
    {
      "source": "/api/neighborhoods(/(.*))?",
      "destination": "/api/neighborhoods.ts"
    },
    {
      "source": "/api/enquiries(/(.*))?",
      "destination": "/api/enquiries.ts"
    },
    {
      "source": "/api/developments(/(.*))?",
      "destination": "/api/developments.ts"
    },
    {
      "source": "/api/developers(/(.*))?",
      "destination": "/api/developers.ts"
    },
    {
      "source": "/api/banner-highlights(/(.*))?",
      "destination": "/api/banner-highlights.ts"
    },
    {
      "source": "/api/agents(/(.*))?",
      "destination": "/api/agents.ts"
    },
    {
      "source": "/api/articles(/(.*))?",
      "destination": "/api/articles.ts"
    },
    {
      "source": "/api/import-properties(/(.*))?",
      "destination": "/api/import-properties.ts"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/client/dist/assets/$1"
    },
    {
      "source": "/favicon.ico",
      "destination": "/client/dist/favicon.ico"
    },
    {
      "source": "/(.*)",
      "destination": "/client/dist/index.html"
    }
  ]
}
